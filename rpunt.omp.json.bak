        {
          "type": "envvar",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "properties": {
            "var_name": "AWS_ENV"
          },
          "style": "diamond",
          "trailing_diamond": ""
        }
      ],
      "type": "prompt"
    },
    // {
    //   "type": "newline"
    // },
    {
      "newline": true,
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "#e2e2d5",
          "properties": {
            "prefix": "",
            "text": "$"
          },
          "style": "plain",
          "type": "text"
        }
      ]
    }
